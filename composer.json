{
    "name": "akkurate/laravel-contact",
    "description": "Contact package for Laravel applications",
    "type": "library",
    "authors": [
        {
            "name": "Akkurate",
            "email": "hello@subvitamine.com",
            "homepage": "https://www.akkurate.io",
            "role": "Company"
        }
    ],
    "support": {
        "docs": "https://github.com/akkurateio/documentation"
    },
    "require": {
        "akkurate/laravel-back-components": "^2.0",
        "akkurate/laravel-core": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0"
    },
    "autoload" : {
        "psr-4": {
            "Akkurate\\LaravelContact\\": "src",
            "Akkurate\\LaravelContact\\Database\\Seeders\\": "database/seeders",
            "Akkurate\\LaravelContact\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Akkurate\\LaravelContact\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Akkurate\\LaravelContact\\LaravelContactServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:subvitaminetm/laravel-core.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:subvitaminetm/laravel-carbon-icons.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:subvitaminetm/laravel-back-components.git"
        }
    ]
}
